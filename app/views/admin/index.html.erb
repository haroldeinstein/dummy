<%= content_for :stylesheets do %>
  <%= stylesheet_link_tag :welcome %>
<% end %>

<div id="welcome" class="w1064">
  <table cellpadding="0" cellspacing="0" border="0" width="100%" height="90%">
    <tbody>
      <tr>
        <td width="100%" height="100%" valign="middle">
          <div class="welcome-content">
            <div class="logo">
            </div>

            <div id="navbar" class="clearfix">
              <ul class="nav-item memoir">
                <li>
                  <h2 class="home"><a href="/admin/the-special-ones">the special ones</a></h2>
                </li>
              </ul>
              <ul class="nav-item contact">
                <li>
                  <h2 class="home"><a href="admin/hey">hey, dummy</a></h2>
                </li>
              </ul>
              <ul class="nav-item directors first">
                <% for director in @directors %>
                  <li><h2 class="home"><%= link_to director.name.downcase, admin_director_path(director) %></h2></li>
                <% end %>
                <li class="new-director"><div class="add"></div><h3>new director</h3></li>
              </ul>
            </div>

          </div>
        </td>
      </tr>
    </tbody>
  </table>

  <h2 class="home news"><a href="/news">news</a></h2>

</div>

