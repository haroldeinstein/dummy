#director {
  height: 50.0em;
}

#videos-list {
  width: 34%;
  height: 100%;
  float: left;
  text-align: center;
}

#videos-list a {
  color: #fdb5af;
  text-decoration: underline;
}

#videos-list h4 {
  padding: 8px 0px;
  text-transform: lowercase;
}

#videos-container {
  float: right;
  width: 66%;
  margin-top: 4.0em;
  height: 42.0em;
}

#video-player {
  width: 100%;
  box-sizing: border-box;
  border: 1px solid #fff;
  padding-bottom: 56.7%;
  overflow: scroll;
}

.video {
  width: 22.1em;
  float: left;
  border: 1px solid #fff;
  box-sizing: border-box;
}

.video img {
  width: 100%;
}

