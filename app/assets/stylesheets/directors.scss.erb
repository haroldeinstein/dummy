#videos-list {
  width: 34%;
  height: 42.0em;
  float: left;
  text-align: center;
}

#videos-list a {
  color: #fdb5af;
  text-decoration: underline;
}

#videos-list h4 {
  padding: 8px 0px;
  text-transform: lowercase;
}

#videos-container {
  float: right;
  width: 66%;
  height: 42.0em;
  overflow: hidden;
  background: #ccc;
}

.video {
  width: 35.0em;
  float: left;
  border: 1px solid #fff;
}

.video img {
  width: 100%;
}

