.news-index #main-container, .directors-index #main-container {
  max-width: 113.0em;
}

#director {
  height: 50.0em;
}

#videos-list {
  width: 23%;
  height: 100%;
  float: left;
  text-align: center;
}

#videos-list a {
  color: #fdb5af;
  text-decoration: underline;
}

#videos-list h3 {
  padding: 6px 0px;
  text-transform: lowercase;
  cursor: pointer;
}

#videos-container {
  float: right;
  width: 70%;
  margin-top: 3.0em;
  height: 44.8em;
  overflow: hidden;
}

#video-player {
  width: 100%;
  box-sizing: border-box;
  padding-bottom: 56.7%;
  position: relative;
}

iframe {
  position: absolute;
  top: 0em;
  left: 0em;
}

